{
  "manifest_version": 3,
  "name": "YouTube Downloader",
  "version": "1.0",
  "description": "Download YouTube videos with custom resolution",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": ["activeTab"]
}
